{
	"name": "perpel/yii2-lte",
    "description": "yii2 lte",
	"keywords": [  
		"yii2",  
		"admin lte"
	  ],  
    "authors": [
        {
            "name": "kongdehui",
            "email": "572821520@qq.com"
        }
    ],
	"homepage": "https://github.com/perpel/yii2-lte",
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/perpel/yii2-lte"
    },
    "require": {
		"yiisoft/yii2-bootstrap": "*",
		"bower-asset/admin-lte": "*"
    },
    "autoload": {
        "psr-4": {
            "yiip\\lte\\": "src/"
        }
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
